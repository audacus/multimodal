# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Model Configuration (Qwen3 Omni)
MODEL_NAME=Qwen/Qwen3-Omni
MODEL_API_BASE=http://your-digitalocean-gpu-ip:8000/v1
MODEL_API_KEY=your_api_key_or_none

# LLM Settings
MAX_TOKENS=2048
TEMPERATURE=0.7
TOP_P=0.9

# MCP Server Configuration (comma-separated)
MCP_SERVERS_CONFIG=config/mcp_servers.json

# Application Settings
LOG_LEVEL=INFO
MAX_CONVERSATION_HISTORY=10
ENABLE_MEMORY=true

# DigitalOcean GPU Settings
CUDA_VISIBLE_DEVICES=0
TORCH_DTYPE=float16
